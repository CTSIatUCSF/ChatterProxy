<?xml version="1.0" encoding="UTF-8"?>
<Module>
    <ModulePrefs
            title="Person Id Collector">
        <Require feature="pubsub" />
    </ModulePrefs>
    <Content type="html"><![CDATA[<!--HTML-->
    <!-- #includes -->
    <!DOCTYPE html>
<div id="output">
</div>
<script type="text/javascript">
function callback(sender, message) {
  var data = gadgets.json.parse(message);
  document.getElementById("output").innerHTML = data.data.personIds;
};
gadgets.pubsub.subscribe("JSONPersonIds", callback);
</script>
]]>
</Content>
</Module>
