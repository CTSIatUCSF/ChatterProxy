						});
						
					gadgets.window.adjustHeight();
				}
				renderNav();
				
				function renderParams(){
					var params = gadgets.views.getParams();
					var ul = $('<ul/>');
					var c = 0;
					console.log( params );
					
					
					for(p in params){
						$(ul).append( '<li><label>' + p + ' =</label><span class="pvalue">'+params[p]+'</span></li>' );
						c++;	
					}
					if(c>0){
